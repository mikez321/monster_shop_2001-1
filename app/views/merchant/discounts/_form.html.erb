<%= form_for @discount, url: url, method: method do |f| %>
  <%= f.label :name  %>
  <%= f.text_field :name  %><br>
  <%= f.label :description  %>
  <%= f.text_field :description  %><br>
  <%= f.label :amount  %>
  <%= f.text_field :amount  %><br>
  <%= f.label :quantity  %>
  <%= f.text_field :quantity  %><br>
  <%= f.submit %>
<% end %>
